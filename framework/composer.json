{
    "name": "typerocket/typerocket",
    "description": "TypeRocket for WordPress.",
    "keywords": ["framework", "typerocket"],
    "license": "GPL",
    "homepage": "http://typerocket.com",
    "authors": [
        {
            "name": "Kevin Dees",
            "email": "kevin.dees2@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "typerocket/core": "3.0.*",
        "abraham/twitteroauth": "^0.7.4",
        "mpdf/mpdf": "^7.1",
        "dompdf/dompdf": "^0.8.2",
        "phpclasses/numverify": ">=0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "php galaxy config:seed"
        ]
    },
    "minimum-stability": "stable",
    "repositories":
    [
        {
            "type": "composer",
            "url": "https:\/\/www.phpclasses.org\/"
        },
        {
            "packagist": false
        }
    ]
}
